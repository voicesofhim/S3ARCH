{
  "name": "s3arch-monorepo",
  "version": "1.0.0",
  "description": "S3ARCH - Predictive Intelligence Test Monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:s3arch": "cd apps/s3arch && npm run dev",
    "dev:tim3": "cd apps/tim3 && npm run dev",
    "build:s3arch": "cd apps/s3arch && npm run build",
    "build:tim3": "cd apps/tim3 && npm run build",
    "build:all": "npm run build:s3arch && npm run build:tim3",
    "install:all": "npm install && npm run install:workspaces",
    "install:workspaces": "npm install --workspaces"
  },
  "devDependencies": {
    "typescript": "~5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

